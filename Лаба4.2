import math
def so(b):
    for i in range(1,len(b)):
        k=b[i]
        j=i-1
        while j>=0 and k<b[j]:
            b[j+1]=b[j]
            j=j-1
        b[j+1]=k
def bus(a):
    ma=max(a)
    s=ma/len(a)
    b=[]
    for i in range(len(a)):
        b.append([])
    for i in range(len(a)):
        j= math. floor(a[i]/s)
        if j!=len(a):
            b[j].append(a[i])
        else:
            b[len(a)-1].append(a[i])
    for i in range(len(b)):
        so(b[i])
    final=[]
    for i in range (len(b)):
        final=final+b[i]
    return final
n=int(input("Введите колличество элементов:"))
a=[]
for i in range(n):
    a.append(int(input("Введите элеиент:")))
print(bus(a))
